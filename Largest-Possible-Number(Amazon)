import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static String FindLargestNumberByJoining(Integer [] array){
        Arrays.sort(array, new Comparator<Integer>(){
            public int compare(Integer i1, Integer i2){
                Integer first_num = Integer.parseInt(i1.toString()+i2.toString());
                Integer second_num = Integer.parseInt(i2.toString()+i1.toString());
                if(first_num > second_num) 
                    return -1;
                return 1;
            }
        });
        
        String result = "";
        for(Integer num : array)
            result+=num+"";
        return result;
    }
    
    
    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        
        Integer [] array = new Integer[n];
        for(int i=0;i<n;i++)
            array[i]=sc.nextInt();
       
        System.out.println(Long.parseLong(FindLargestNumberByJoining(array)));
    }
}
